<h1>Import CSV</h1>

<h5><%= csv_headers %></h5>
<%= form_tag contacts_path do %>
  <div>
    <%= label_tag :name %>
    <%= select_tag :name, options_for_select(csv_headers, csv_headers[0]) %>
  </div>
  <div>
    <%= label_tag :birth_date %>
    <%= select_tag :birth_date, options_for_select(csv_headers, csv_headers[1]) %>
  </div>
  <div>
    <%= label_tag :phone %>
    <%= select_tag :phone, options_for_select(csv_headers, csv_headers[2]) %>
  </div>
  <div>
    <%= label_tag :address %>
    <%= select_tag :address, options_for_select(csv_headers, csv_headers[3]) %>
  </div>
  <div>
    <%= label_tag :card_number %>
    <%= select_tag :card_number, options_for_select(csv_headers, csv_headers[4]) %>
  </div>
  <div>
    <%= label_tag :email %>
    <%= select_tag :email, options_for_select(csv_headers, csv_headers[5]) %>
  </div>
  <%= submit_tag :submit %>
<% end %>